{
  "dashboard": {
    "id": null,
    "title": "URL Shortener Metrics",
    "tags": ["url-shortener", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Requests per Second",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{service}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Response Time (95th percentile)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{service}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 3,
        "title": "Short URLs Created",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(short_url_created_total[5m])"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Redirects Total",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(redirects_total[5m])"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8}
      },
      {
        "id": 5,
        "title": "Database Queries",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(database_queries_total[5m])",
            "legendFormat": "{{operation}}"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8}
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 1,
    "uid": "url-shortener-dashboard"
  }
}